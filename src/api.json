{
  "api": {
    "GET|/user": {
      "serviceName": "getUserList",
      "description": "获取用户列表",
      "headers": {
        "content-type": "application/json"
      },
      "params": {
        "userName": {
          "type": "string",
          "description": "用户名\n支持中文/英文字母/数字",
          "required": false
        },
        "userId": {
          "type": "string",
          "description": "用户id",
          "required": true
        },
        "userList": {
          "type": "object",
          "array": true,
          "description": "用户id列表",
          "properties": {
            "id": {
              "description": "用户id",
              "type": "string"
            },
            "userName": {
              "type": "string",
              "description": "用户名\n支持中文/英文字母/数字",
              "required": false
            }
          }
        }
      },
      "responseType": "json",
      "response": {
        "type": "object",
        "properties": {
          "total": {
            "type": "number",
            "description": "数据总量"
          },
          "list": {
            "type": "object",
            "array": true,
            "description": "用户列表",
            "properties": {
              "userId": {
                "type": "string",
                "description": "用户ID"
              },
              "userName": {
                "type": "string",
                "description": "用户名"
              },
              "age": {
                "type": "number",
                "description": "年龄"
              }
            }
          }
        }
      }
    },
    "POST|/user": {
      "serviceName": "updateUser",
      "description": "更新用户信息",
      "params": {
        "userId": {
          "type": "string",
          "description": "用户ID",
          "required": true
        },
        "userName": {
          "type": "string",
          "description": "用户名\n支持中文/英文字母/数字",
          "required": true
        },
        "age": {
          "type": "number",
          "description": "年龄",
          "required": false
        }
      },
      "responseType": "json",
      "response": {
        "type": "string",
        "description": "success | error"
      }
    },
    "PUT|/user": {
      "serviceName": "addUser",
      "description": "添加用户信息",
      "params": {
        "userName": {
          "type": "string",
          "description": "用户名\n支持中文/英文字母/数字",
          "required": true
        },
        "age": {
          "type": "number",
          "description": "年龄",
          "required": false
        }
      },
      "responseType": "json",
      "response": {
        "type": "string",
        "description": "success | error"
      }
    },
    "DELETE|/user": {
      "serviceName": "deleteUser",
      "description": "通过ID删除用户信息",
      "params": {
        "userId": {
          "type": "string",
          "description": "用户ID",
          "required": true
        }
      },
      "responseType": "json",
      "response": {
        "type": "string",
        "description": "success | error"
      }
    },
    "GET|/user/{userId}": {
      "serviceName": "getUserById",
      "description": "获取用户详情",
      "headers": {},
      "params": {
        "userId": {
          "type": "string",
          "required": true
        }
      },
      "response": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "用户ID"
          },
          "userName": {
            "type": "string",
            "description": "用户名"
          },
          "age": {
            "type": "number",
            "description": "年龄"
          }
        }
      }
    }
  },
  "config": {}
}